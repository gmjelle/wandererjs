{
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "name": "wanderer.js",
  "version": "0.0.7",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yaakov123/wandererjs.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack watch",
    "patch": "npm version patch",
    "build:esm": "cross-env NODE_ENV=production webpack --env production --config webpack-esm.config.js",
    "build:umd": "cross-env NODE_ENV=production webpack --env production",
    "build": "npm run build:umd && npm run build:esm",
    "serve": "serve -l 5000 ./",
    "test": "npx playwright test",
    "test:serve": "concurrently 'npm run serve' 'sleep 5 && npx playwright test'",
    "build:test": "npm run build && npm run test:serve",
    "lint": "eslint --ext js,vue,ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.5",
    "@popperjs/core": "^2.10.2",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "postcss-import": "^14.0.2",
    "ts-loader": "^9.2.6",
    "vue": "^3.2.21",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@playwright/test": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vue/compiler-sfc": "^3.2.21",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.3.1",
    "css-loader": "^6.5.1",
    "eslint": "^8.2.0",
    "file-loader": "^6.2.0",
    "jest-playwright-preset": "^1.7.0",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^2.4.4",
    "playwright": "^1.16.3",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "serve": "^13.0.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^2.2.19",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "vue-loader": "^16.8.3",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
